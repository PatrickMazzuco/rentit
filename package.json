{
  "name": "rentit",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "dev": "nodemon --ext ts --exec \"npm run run:dev\"",
    "debug": "nodemon --ext ts --exec \"npm run run:debug\"",
    "run:dev": "npm run build:dev && node dist/main.js",
    "run:debug": "npm run build:dev && node --inspect dist/main.js",
    "build": "swc src -d dist",
    "build:dev": "swc src -s --out-dir dist",
    "start": "npm run build && node dist/main",
    "clean": "rm -rf dist",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:unit": "dotenv -e .env.test.local mocha -r ts-node/register src/**/*.spec.ts",
    "test:integration": "dotenv -e .env.test.local mocha -r ts-node/register src/**/*.int-spec.ts",
    "migration:run:development": "prisma migrate dev",
    "migration:run:test": "dotenv -e .env.test.local prisma migrate dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^5.1.0",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.189",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-subset": "^1.3.3",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "axios": "^0.27.2",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "prisma": "^3.14.0",
    "sinon": "^14.0.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.8.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4"
  }
}
